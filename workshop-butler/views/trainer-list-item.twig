<div class="wsb-trainer-tile" data-trainer-obj
     data-trainer-location="{{ trainer.country_code }}"
     data-trainer-language="{{ trainer.languages |join(',') }}"
     data-trainer-rating={{ trainer.public_stats.rating }}
     data-trainer-badge="{{ trainer.name_of_badges |join(',') }}"
     data-trainer-trainer="{{ trainer.full_name }}">
    <div class="wsb-trainer-tile__box">
        {{ content | raw }}
    </div>
</div>
